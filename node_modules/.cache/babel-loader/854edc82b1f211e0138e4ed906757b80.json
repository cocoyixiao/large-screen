=> {\n    let info = {}\n    let { status, statusText, data } = error.response\n    if (!error.response) {\n      info = {\n        code: 5000,\n        msg: 'Network Error'\n      }\n    } else {\n      // 此处整理错误信息格式\n      info = {\n        code: status,\n        data: data,\n        msg: statusText\n      }\n    }\n    return Promise.reject(info)\n  }\n)\n\nexport default function() {\n  return http\n}\n"],"mappings":"AAAA;AACA;AACA;AACA;AACA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,QAAQ,QAAQ,cAAc;AACvC;AACA,IAAIC,IAAI,GAAGF,KAAK,CAACG,MAAM,CAAC;EACtBC,OAAO,EAAEC,OAAO,CAACC,GAAG,CAACC,gBAAgB;EAAE;EACvCC,OAAO,EAAE;AACX,CAAC,CAAC;;AAEF;AACAN,IAAI,CAACO,QAAQ,CAACC,OAAO,CAACC,IAAI,CAAC,cAAc,CAAC,GAAG,mCAAmC;AAChFT,IAAI,CAACO,QAAQ,CAACG,eAAe,GAAG,IAAI;;AAEpC;AACAV,IAAI,CAACW,YAAY,CAACC,OAAO,CAACC,GAAG,CAC3BC,MAAM,IAAI;EACR,MAAMC,KAAK,GAAGhB,QAAQ,EAAE;EACxB,IAAIgB,KAAK,EAAE;IACTD,MAAM,CAACN,OAAO,CAAC,eAAe,CAAC,GAAGO,KAAK;EACzC;EACA,OAAOD,MAAM;AACf,CAAC,EACDE,KAAK,IAAI;EACP;EACA,OAAOC,OAAO,CAACC,MAAM,CAACF,KAAK,CAAC;AAC9B,CAAC,CACF;;AAED;AACAhB,IAAI,CAACW,YAAY,CAACQ,QAAQ,CAACN,GAAG,CAC5BM,QAAQ,IAAI;EACV,IAAI;IAAEC;EAAK,CAAC,GAAGD,QAAQ;EACvB,IAAIC,IAAI,KAAI,GAAG,EAAE;IACf,OAAOA,IAAI;EACb,CAAC,MAAM;IACL,OAAOA,IAAI;EACb;AACF,CAAC,EACDJ,KAAK,IAAI;EACP,IAAIK,IAAI,GAAG,CAAC,CAAC;EACb,IAAI;IAAEC,MAAM;IAAEC,UAAU;IAAEH;EAAK,CAAC,GAAGJ,KAAK,CAACG,QAAQ;EACjD,IAAI,CAACH,KAAK,CAACG,QAAQ,EAAE;IACnBE,IAAI,GAAG;MACLG,IAAI,EAAE,IAAI;MACVC,GAAG,EAAE;IACP,CAAC;EACH,CAAC,MAAM;IACL;IACAJ,IAAI,GAAG;MACLG,IAAI,EAAEF,MAAM;MACZF,IAAI,EAAEA,IAAI;MACVK,GAAG,EAAEF;IACP,CAAC;EACH;EACA,OAAON,OAAO,CAACC,MAAM,CAACG,IAAI,CAAC;AAC7B,CAAC,CACF;AAED,eAAe,YAAW;EACxB,OAAOrB,IAAI;AACb"}]}